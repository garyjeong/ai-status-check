# Environment variables for AI Status Monitoring Dashboard
# Copy this file to .env.local and fill in your values

# Development Configuration
VITE_APP_TITLE="AI 서비스 상태 모니터링"
VITE_APP_DESCRIPTION="AI 서비스들의 실시간 상태를 모니터링하는 대시보드"

# API Configuration
VITE_API_PROXY_URL=http://localhost:5173
VITE_POLLING_INTERVAL=30000

# External API Endpoints (used by Vite proxy)
# OpenAI
VITE_OPENAI_STATUS_URL=https://status.openai.com/api/v2/status.json

# Anthropic
VITE_ANTHROPIC_STATUS_URL=https://status.anthropic.com/api/v2/status.json

# Cursor (if they have a status API)
VITE_CURSOR_STATUS_URL=https://api.cursor.com/status

# Google AI Studio
VITE_GOOGLE_AI_STATUS_URL=https://status.cloud.google.com/incidents.json

# Additional Configuration
# Add your custom environment variables here

# Feature Flags
VITE_ENABLE_DARK_MODE=true
VITE_ENABLE_AUTO_REFRESH=true
VITE_ENABLE_NOTIFICATIONS=false

# Development Settings
VITE_DEV_MODE=true
VITE_DEBUG_API=false 